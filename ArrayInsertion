#include <iostream>
using namespace std;
int main()
{
    int arr[50]={1,2,3,4,5,6};
    int sizee=6;
    int index, element;
    cout<<"Enter a index to enter element between array: ";
    cin>>index;
    cout<<"Enter element: ";
    cin>>element;
    for(int i=5; i>=index; i--)
    {
        arr[i+1]=arr[i];
    }
    arr[index]=element;
    sizee++;
    cout<<"The elements of array is: ";
    for(int i=0; i<7; i++)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}
