#include <iostream>
using namespace std;
int main()
{
    int arr[6]={10,12,5,20,34,1};
    int wordLength=sizeof(arr[1]);
    int *baseAddress=arr;
    //Find address of i index
    for(int i=0; i<6; i++)
    {
        cout<<"Address of "<<i<<" index element is: ";
        cout<<baseAddress+i<<endl;
    }

    return 0;
}
