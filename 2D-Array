#include <iostream> 
using namespace std; 
  
int main() 
{ 
  
    int count = 1; 
    
    int array1[2][3]; 
  
   
    for (int i = 0; i < 2; i++) { 
        for (int j = 0; j < 3; j++) { 
            array1[i][j] = count; 
            count++; 
        } 
    } 
  
    for (int i = 0; i < 2; i++) { 
        for (int j = 0; j < 3; j++) { 
            cout << array1[i][j] << " "; 
        } 
        cout << endl; 
    } 
  
    return 0; 
}
