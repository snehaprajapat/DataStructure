#include <iostream>
using namespace std;
int main()
{
    int arr[10]={1,2,3,4,5,6,7,8};
    int removee;
    cout<<"Enter index to delete element: ";
    cin>>removee;
    for(int i=removee;i<7 ;i++)
    {
        arr[i]=arr[i+1];
    }
    for(int i=0; i<7;i++)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}
