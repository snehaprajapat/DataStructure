#include <iostream>
using namespace std;
int main()
{
    int arr[50];
    int Size;
    cout<<"How many Elements you want to enter in array? ";
    cin>>Size;
    for(int i=0; i<Size; i++)
    {
        cout<<"Enter element "<<i+1<<": ";
        cin>>arr[i];
    }
    cout<<"The elements of array is: ";
    for(int i=0; i<Size; i++)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}
